{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.js,.jsx,.json}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"src/**/*{.js,.jsx,.json,.css}\"",
    "lint:js": "eslint \"./src/**/*.{js,jsx}\"",
    "lint:styles": "stylelint  \"./src/**/*.{js,jsx,css}\"",
    "lint:all": "npm run prettier:check && npm run lint:styles && npm run lint:js",
    "fix:js": "eslint 'src/**/*.{js,jsx}' --fix",
    "fix:stylelint": "stylelint 'src/**/*.{js,jsx,pcss}' --fix",
    "fix:all": "npm run fix:js && npm run prettier:fix && npm run fix:stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && git add",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:all"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.3",
    "prettier": "2.0.4",
    "stylelint": "^13.3.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
